{
    "name": "vaweto/test-repo",
    "description": "This is my package test-repo",
    "keywords": [
        "vaweto",
        "test-repo"
    ],
    "homepage": "https://github.com/vaweto/test-repo",
    "license": "MIT",
    "authors": [
        {
            "name": "Vagelis Bismpikis",
            "email": "vagelisbisbikis@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Vaweto\\TestRepo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vaweto\\TestRepo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
